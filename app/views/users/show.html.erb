<% provide(:title, current_user.name) %>
<%= current_user.name %>

<% if current_user.role? :learner %>
<ul class="classrooms">
  <% current_user.classrooms.each do |classroom| %>
  <li>
    <%= link_to classroom.course.title, [classroom, classroom.pages.where("title = 'Forside'").first] %> 
  </li>
  
 
  <% end %>
</ul>

<% end %> 
